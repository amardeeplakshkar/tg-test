datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id           Int      @id @map("_id") @default(autoincrement())
  first_name   String
  last_name    String?
  username     String?
  language_code String
  is_premium   Boolean  @default(false)
}
